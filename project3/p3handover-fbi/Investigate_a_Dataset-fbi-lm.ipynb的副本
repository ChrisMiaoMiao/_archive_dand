    "# bar chart\n",
    "fig = plt.figure(figsize=(16,16))\n",
    "ax = fig.add_subplot(1,1,1)\n",
    "ind = np.arange(len(dfq1.iloc[0]))\n",
    "# 要想微调位置要设置个变量ind\n",
    "w = 0.4\n",
    "# bar的宽度也是可以设定的\n",
    "\n",
    "# 如果使用循环的话，颜色要使用编码才可以有所区分，先放弃\n",
    "'''\n",
    "i = 0\n",
    "while i < len(dfq1.index):\n",
    "    ax.bar(ind, dfq1.iloc[i], color = 'r', alpha = 0.7, width = w, label = dfq1.index[i])\n",
    "    i = i + 1\n",
    "'''\n",
    "\n",
    "ax.bar(ind, dfq1.iloc[0], color = 'y', alpha = 0.7, width = w, label = dfq1.index[0])\n",
