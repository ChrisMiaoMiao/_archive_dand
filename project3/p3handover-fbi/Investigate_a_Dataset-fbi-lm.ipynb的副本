    "mean = dfsm25k.groupby('has_homepage').mean()\n",
    "mean\n",
    "# 可以看出来区别还是很大的"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "# 我们来画图试试，结果相差太多，前两个基本看不到啊\n",
    "dfsm25k.groupby('has_homepage').mean().iloc[0].plot(\n",
    "    kind = 'bar', alpha = 0.7, color = 'b');"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 把数据都搞到一个数量级好了\n",
    "mean_edited = mean.copy()\n",
    "# 需要注意如果不用.copy()的话修改mean_edited 也会修改mean\n",
    "mean_edited['budget_adj'] = mean['budget_adj'] / 10000000\n",
    "mean_edited['revenue_adj'] = mean['revenue_adj'] / 10000000\n",
    "mean_edited"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "mean_edited.iloc[1] / mean_edited.iloc[0]\n",
    "# 我们来看看变化有多少，pop和rev的增幅都超过了50%"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 堆叠起来看看，不是特别理想\n",
    "mean_edited.iloc[0].plot(kind = 'bar', color = 'r', alpha = 0.3)\n",
    "mean_edited.iloc[1].plot(kind = 'bar', color = 'b', alpha = 0.3);"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 那只好认真的画图了，开始想到了折线图，但是这4个指标没有延续的关系\n",
    "# 折线图比较适合画x轴为时间的数据\n",
    "sns.set(context = 'notebook', style = 'darkgrid')\n",
    "# 使用sns设置底色要放在最前面\n",
    "fig = plt.figure(figsize=(8,8))\n",
    "# 如果使用是import的是matplotlib 而不是 matplotlib.pyplot\n",
    "# 调用的时候会有区别\n",
    "ax = fig.add_subplot(1,1,1)\n",
    "ind = np.arange(len(mean_edited.iloc[0]))\n",
    "# 要想微调位置要设置个变量ind\n",
    "w = 0.3\n",
    "# bar的宽度也是可以设定的\n",
    "ax.bar(ind, mean_edited.iloc[0], color = 'r',\n",
    "       alpha = 0.7, width = w, label = \"Homepage No\")\n",
    "ax.bar(ind+w, mean_edited.iloc[1], color = 'b',\n",
    "       alpha = 0.7, width = w, label = \"Homepage Yes\")\n",
    "#ind是bar的位置，datared是输入的数据，多个width是bar的宽度\n",
    "ax.legend(loc = 'best', fontsize = 12)\n",
    "ax.set_xticks(ind+w/2)\n",
    "# 设定x轴标签位置，在正中间\n",
    "# 根据你要堆叠的数据设定\n",
    "mean_lable = ['Popularity','Vote Average',\n",
    "              'Budget/10^7', 'Revenue/10^7']\n",
    "# 把标签设定为比较友好的方式\n",
    "# 也可以导出index处理，比写死了好，有没有兴趣优化一下？\n",
    "ax.set_xticklabels(mean_lable)\n",
    "# 将分类标签作为x标签\n",
    "\n",
    "ax.set_ylabel('Values', fontsize = 16)\n",
    "ax.set_xlabel('Features', fontsize = 16)\n",
    "fig.suptitle('Compare Features on Homepage or Not',\n",
    "             fontsize = 24, color = 'red');\n",
    "# 记住在最后的语句后面加个；就不会显示图之外的信息，比较友好"
   ]
  },
  {
